#include<bits/stdc++.h>
using namespace std;
int main(){
	int a[20],element,n,flag=0,i;
	cout<<"How many element u want to insert\n";
	cin>>n;
	cout<<"Enter the array\n";
	for(i=0;i<n;i++)
	cin>>a[i];
	cout<<"Enter the element to be searching\n";
	cin>>element;
	for(i=0;i<n;i++){
		if(a[i]==element){
			flag=1;
			}
	}
	if(flag==1){
		cout<<"Element is found in this list\n";
	}else{
		cout<<"Element is not found in this list\n";
	}
	return 0;
}
